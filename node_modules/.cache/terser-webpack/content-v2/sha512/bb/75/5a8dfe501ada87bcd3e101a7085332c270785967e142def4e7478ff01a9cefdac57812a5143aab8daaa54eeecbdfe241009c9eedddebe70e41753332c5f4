            return 'escape';\n                default:\n                    return keyName;\n            }\n        }\n    }\n    KeyEventsPlugin.fac = function KeyEventsPlugin_Factory(t) { return new (t || KeyEventsPlugin)(inject(common_DOCUMENT)); };\n    KeyEventsPlugin.prov = /*@__PURE__*/ defineInjectable({ token: KeyEventsPlugin, factory: KeyEventsPlugin.fac });\n    return KeyEventsPlugin;\n})();\nfunction getEventKey(event) {\n    let key = event.key;\n    if (key == null) {\n        key = event.keyIdentifier;\n        // keyIdentifier is defined in the old draft of DOM Level 3 Events implemented by Chrome and\n        // Safari cf\n        // http://www.w3.org/TR/2007/WD-DOM-Level-3-Events-20071221/events.html#Events-KeyboardEvents-Interfaces\n        if (key == null) {\n            return 'Unidentified';\n        }\n        if (key.startsWith('U+')) {\n            key = String.fromCharCode(parseInt(key.substring(2), 16));\n            if (event.location === DOM_KEY_LOCATION_NUMPAD && _chromeNumKeyPadMap.hasOwnProperty(key)) {\n                // There is a bug in Chrome for numeric keypad keys:\n                // https://code.google.com/p/chromium/issues/detail?id=155654\n                // 1, 2, 3 ... are reported as A, B, C ...\n                key = _chromeNumKeyPadMap[key];\n            }\n        }\n    }\n    return _keyMap[key] || key;\n}\nlet platform_browser_DomSanitizer = /*@__PURE__*/ (() => {\n    class DomSanitizer {\n    }\n    DomSanitizer.fac = function DomSanitizer_Factory(t) { return new (t || DomSanitizer)(); };\n    DomSanitizer.prov = /*@__PURE__*/ defineInjectable({ factory: function DomSanitizer_Factory() { return inject(platform_browser_DomSanitizerImpl); }, token: DomSanitizer, providedIn: \"root\" });\n    return DomSanitizer;\n})();\nfunction domSanitizerImplFactory(injector) {\n    return new platform_browser_DomSanitizerImpl(injector.get(common_DOCUMENT));\n}\nlet platform_browser_DomSanitizerImpl = /*@__PURE__*/ (() => {\n   